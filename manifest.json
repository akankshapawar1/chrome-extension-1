{
    "manifest_version": 3,
    "name": "Daily Documentation Bite",
    "version": "1.0",
    "description": "Get daily snippets of web technology documentation.",
    "permissions": ["storage", "activeTab"],
    "background": {
      "service_worker": "dist/background.bundle.js",
      "type": "module"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  